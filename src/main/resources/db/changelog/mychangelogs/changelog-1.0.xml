   </changeSet>
 
 -->
 <changeSet  author="root"  id="3">  
    <insert   
            schemaName="project"  
            tableName="person">  
       <column  name="id"  value="5"/>  
        <column  name="name"  value="tom"/>  
    </insert>  
</changeSet>


